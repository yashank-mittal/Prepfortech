//Reverse Array

#include<bits/stdc++.h>
using namespace std;

void reverseArray(int a[],int n){
    int i = 0;
    int j = n - 1;
    while(i<j){
        int temp = a[i];
        a[i] = a[j];
        a[j] = temp;
        i++;
        j--;
    }

    for(int i=0;i<n;i++){
        cout<<a[i]<<" ";
    }

}

int main(){
    int a[] = {1,2,3,4,5};
    int n = sizeof(a)/sizeof(a[0]);
    reverseArray(a,n);
    return 0;
}
